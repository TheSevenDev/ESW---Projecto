@model CIMOB_IPS.Models.Student


<form asp-action="EditProfileStudent">
    <div asp-validation-summary="ModelOnly" class="error-message"></div>
    @Html.HiddenFor(model => Model.IdAccount)
    @Html.HiddenFor(model => Model.Name)
    <div class="form-group">
        <label asp-for="Telephone" class="control-label"></label>
        <input asp-for="Telephone" class="form-control" />
        <span asp-validation-for="Telephone" class="error-message"></span>
    </div>
    <div class="form-group">
        <label asp-for="StudentNum" class="control-label"></label>
        <input asp-for="StudentNum" class="form-control" />
        <span asp-validation-for="StudentNum" class="error-message"></span>
    </div>
    <div class="form-group">
        <label asp-for="Address" class="control-label"></label>
        <input asp-for="Address" class="form-control" />
        <span asp-validation-for="Address" class="error-message"></span>
    </div>
    <div class="form-group">
        <input type="submit" value="Guardar" class="btn btn-default" />
    </div>
</form>

